#include <iostream>
#include <vector>
using namespace std;

int main() {
	vector<int> v;
	vector<int>::iterator it;
	v.push_back(10);
	v.push_back(20);
	v.push_back(30);
	v.push_back(40);

	for(it = v.begin(); it != v.end(); it++)
	{
		cout<<*it<<endl;
	}

	for(int i = 0; i < 5; i++)
	{
		cout<<v[i]<<endl;
	}

	return 0;
}
